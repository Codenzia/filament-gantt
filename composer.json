{
  "name": "codenzia/filament-gantt",
  "description": "Filament Gantt page package",
  "keywords": [
    "laravel",
    "filament",
    "db-flow"
  ],
  "homepage": "https://github.com/codenzia/filament-db-flow",
  "authors": [
      {
          "name": "Codenzia",
          "email": "mahmoud@codenzia.com",
          "role": "Developer"
      }
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
